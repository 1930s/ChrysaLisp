(import 'inc/func.inc)
(import 'class/class_pair.inc)

(def-func 'class/pair/deinit)
	;inputs
	;r0 = pair object
	;trashes
	;all but r0, r4

	(vp-push r0)
	(f-call 'ref 'deref '((r0 pair_first)))
	(vp-cpy-ir r4 r0)
	(f-call 'ref 'deref '((r0 pair_second)))
	(vp-pop r0)
	(s-jmp 'pair 'deinit '(r0))

(def-func-end)
