(import 'inc/func.inc)
(import 'class/class_pair.inc)

(def-func 'class/pair/init)
	;inputs
	;r0 = pair object
	;r1 = vtable pointer
	;r2 = first object
	;r3 = second object
	;outputs
	;r1 = 0 if error, else ok

	;save inputs
	(vp-cpy-ri r2 r0 pair_first)
