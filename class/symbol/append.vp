(import 'inc/func.inc)
(import 'class/class_symbol.inc)

(def-func 'class/symbol/append)
	;inputs
	;r0 = symbol object
	;r1 = symbol object
	;outputs
	;r0 = 0 if error, else new symbol object
	;trashes
	;r1-r3, r5-r7

	(f-call 'string 'append '(r0, r1) '(r0))
	(vpif '(r0 != 0))
		(func-path 'class 'symbol)
		(fn-bind _function_ r1)
		(vp-cpy-ri r1 r0 obj_vtable)
	(endif)
	(vp-ret)

(def-func-end)
