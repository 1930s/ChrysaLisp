(import 'inc/func.inc)
(import 'class/symbol/symbol.inc)

(def-func 'class/symbol/create_from_buffer)
	;inputs
	;r0 = buffer pointer
	;r1 = buffer length
	;outputs
	;r0 = 0 if error, else object
	;trashes
	;r1-r3, r5-r7

	(f-call 'string 'create_from_buffer '(r0 r1) '(r0))
	(vpif '(r0 != 0))
		(f-path 'class 'symbol)
		(fn-bind _function_ r1)
		(vp-cpy-ri r1 r0 obj_vtable)
	(endif)
	(vp-ret)

(def-func-end)

