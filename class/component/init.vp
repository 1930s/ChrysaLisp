(import 'inc/func.inc)
(import 'class/class_component.inc)

(def-func 'class/component/init)
	;inputs
	;r0 = component object
	;r1 = vtable pointer
	;outputs
	;r1 = 0 if error, else ok

	;init parent
	(s-call 'component 'init '(r0 r1) '(r1))
	(vpif '(r1 != 0))
		;init myself
		(vp-lea-ir r0 component_slot_list r1)
		(lh-init r1 r2)
	(endif)
	(vp-ret)

(def-func-end)
