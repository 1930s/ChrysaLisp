(import 'sys/func.inc)
(import 'class/stream/class.inc)
(import 'class/slave/class.inc)
(import 'class/integer/class.inc)
(import 'class/lisp/class.inc)

(def-method 'stream 'lisp_available)
	;inputs
	;r0 = lisp object (ptr)
	;r1 = args vector object (ptr)
	;outputs
	;r0 = lisp object (ptr)
	;r1 = return value object (ptr)

	(ptr 'this 'args 'value)
	(ulong 'arg0)

	(push-scope)
	(f-entry 'stream 'lisp_available {this, args})

(errorcases
	(call 'lisp 'env_args_type {this, args, $sig, 1} {_, value})
	(errorifnot {value} 'error1))

	(call 'lisp 'env_args_set {this, args, &arg0, 0})
	(call 'stream 'available {arg0} {_, arg0})
	(call 'integer 'create {arg0} {value})

(vp-label 'exit)
	(f-exit 'stream 'lisp_available {this, value})
	(return)

(errorcases
(vp-label 'error1)
	(call 'lisp 'repl_error {this, "(stream-available stream)", error_msg_wrong_types, args} {_, value})
	(goto 'exit)
	(signature 'sig 'stream))

	(pop-scope-syms)

(def-func-end)

(def-method 'stream 'lisp_write_flush)
	;inputs
	;r0 = lisp object (ptr)
	;r1 = args vector object (ptr)
	;outputs
	;r0 = lisp object (ptr)
	;r1 = return value object (ptr)

	(ptr 'this 'args 'value)
	(ulong 'arg0)

	(push-scope)
	(f-entry 'stream 'lisp_write_flush {this, args})

(errorcases
	(call 'lisp 'env_args_type {this, args, $sig, 1} {_, value})
	(errorifnot {value} 'error1))

	(call 'lisp 'env_args_set {this, args, &arg0, 0})
	(call 'stream 'write_flush {arg0})
	(call 'obj 'ref {arg0} {value})

(vp-label 'exit)
	(f-exit 'stream 'lisp_write_flush {this, value})
	(return)

(errorcases
(vp-label 'error1)
	(call 'lisp 'repl_error {this, "(stream-write-flush stream)", error_msg_wrong_types, args} {_, value})
	(goto 'exit)
	(signature 'sig 'stream))

	(pop-scope-syms)

(def-func-end)
