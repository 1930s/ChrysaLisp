(import 'inc/func.inc)
(import 'class/class_string.inc)

(def-func 'class/string/ref_element)
	;inputs
	;r0 = string object
	;r1 = char index
	;outputs
	;r0 = string object
	;r1 = char string

	(vp-push r0)
	(vp-add-cr string_data r1)
	(f-call 'string 'create_from_buffer '((& r0 r1) 1) '(r1))
	(vp-pop r0)
	(vp-ret)

(def-func-end)
