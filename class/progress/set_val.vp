(import 'inc/func.inc)
(import 'class/class_progress.inc)

(def-func 'class/progress/set_val)
	;inputs
	;r0 = progress object
	;r1 = value
	;outputs
	;r1 = value, clipped to max

	(vpif '(r1 > [r0 + progress_max]))
		(vp-cpy-ir r0 progress_max r1)
	(endif)
	(vp-cpy-ri r1 r0 progress_val)
	(vp-ret)

(def-func-end)
