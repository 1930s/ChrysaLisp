(import 'inc/func.inc)
(import 'class/class_vector.inc)
(import 'class/class_lisp.inc)

(def-func 'class/lisp/func_env)
	;inputs
	;r0 = lisp object
	;r1 = args
	;outputs
	;r0 = lisp object
	;r1 = enviroment

	(ptr 'this 'args)

	(push-scope)
	(retire '(r0, r1) '(this, args))

	(assign '(this -> lisp_enviroment) '(args))
	(func-call 'ref 'ref '(args))

	(expr '(this, args) '(r0, r1))
	(pop-scope)
	(return)

(def-func-end)
