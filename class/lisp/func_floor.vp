(import 'sys/func.inc)
(import 'sys/math/class.inc)
(import 'class/integer/class.inc)
(import 'class/lisp/class.inc)

(def-method 'lisp 'func_floor)
	;inputs
	;r0 = lisp object (ptr)
	;r1 = args vector object (ptr)
	;outputs
	;r0 = lisp object (ptr)
	;r1 = return value object (ptr)

	(ptr 'this 'value)
	(union
		'(ptr 'args)
		'(long 'num))

	(push-scope)
	(entry 'lisp 'func_floor {this, args})

(errorcases
	(call 'lisp 'env_args_type {this, args, $sig, 1} {_, value})
	(errorifnot {value} 'error1))

	(call 'vector 'get_first {args} {_, value})
	(call 'integer 'get_value {value} {_, num})
	(vec-set 0)
	(vec-load 1 {num})
	(vec-floor 1)
	(vec-store 1 {num})
	(call 'integer 'create {num} {value})

(vp-label 'funcexit)
	(exit 'lisp 'func_floor {this, value})
	(return)

(errorcases
(vp-label 'error1)
	(call 'lisp 'repl_error {this, "(floor num)", error_msg_wrong_types, args} {_, value})
	(goto 'funcexit)
	(signature 'sig 'integer))

	(pop-scope-syms)

(def-func-end)
