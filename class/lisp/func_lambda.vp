(import 'sys/func.inc)
(import 'class/lisp/lisp.inc)

(def-func 'class/lisp/func_lambda)
	;inputs
	;r0 = lisp object
	;r1 = args
	;outputs
	;r0 = lisp object
	;r1 = value

	(f-entry 'lisp 'func_lambda '(r2 r0))
	(f-call 'ref 'ref '(r0) '(r0))
	(f-exit 'lisp 'func_lambda '(r2 r0))
	(return)

(def-func-end)
