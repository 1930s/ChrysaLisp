(import 'inc/func.inc)
(import 'class/class_lisp.inc)

(def-func 'class/lisp/deinit)
	;inputs
	;r0 = object
	;trashes
	;all

	(ptr 'this)

	(push-scope)
	(assign '(r0) '(this))

	;deinit myself
	(f-call 'ref 'deref {this->lisp_symbols})
	(f-call 'ref 'deref {this->lisp_enviroment})
	(f-call 'ref 'deref {this->lisp_macros})

	;deinit parent
	(s-call 'lisp 'deinit {this})

	(pop-scope)
	(return)

(def-func-end)
