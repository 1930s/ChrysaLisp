(import 'sys/func.inc)
(import 'class/string/class.inc)
(import 'class/lisp/class.inc)

(def-method 'lisp 'func_match)
	;inputs
	;r0 = lisp object (ptr)
	;r1 = args vector object (ptr)
	;outputs
	;r0 = lisp object (ptr)
	;r1 = return value object (ptr)

	(ptr 'this 'args 'value1 'value2)
	(pptr 'iter_begin 'iter_end)
	(union
		'(pptr 'iter)
		'(uint 'length1 'length2))

	(push-scope)
	(entry 'lisp 'func_match {this, args})

(errorcases
	(call 'lisp 'env_args_type {args, $sig, 2} {value1})
	(errorifnot {value1} 'error1))

	(call 'vector 'get_begin {args} {_, iter_begin})
	(assign {iter_begin[0], iter_begin[ptr_size]} {value1, value2})
	(d-call 'vector 'get_length {value1} {_, length1})
	(d-call 'vector 'get_length {value2} {_, length2})
	(vpif {length1 == length2})
		(call 'vector 'get_iters {value1} {_, iter_begin, iter_end})
		(call 'vector 'get_begin {value2} {_, iter})
		(loop-while {iter_begin != iter_end})
			(assign {*iter_begin, *iter} {value1, value2})
			(assign {iter_begin + ptr_size, iter + ptr_size} {iter_begin, iter})
			(continueif {value1 == value2})
			(call 'obj 'inst_of (cat {value2, @} (f-path 'string 'vtable)) {_, args})
			(gotoifnot {args} 'nomatch)
			(gotoif {ushort(value2->string_data) != char_underscore} 'nomatch)
		(loop-end)
		(assign {this->lisp_sym_t} {value1})
	(else)
	(vp-label 'nomatch)
		(assign {this->lisp_sym_nil} {value1})
	(endif)
	(call 'obj 'ref {value1})

(vp-label 'funcexit)
	(exit 'lisp 'func_match {this, value1})
	(return)

(errorcases
(vp-label 'error1)
	(call 'lisp 'repl_error {this, "(match? list list)", error_msg_wrong_types, args} {_, value1})
	(goto 'funcexit)
	(signature 'sig 'vector 'vector))

	(pop-scope-syms)

(def-func-end)
