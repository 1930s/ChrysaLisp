(import 'sys/func.inc)
(import 'class/string/class.inc)
(import 'gui/points/class.inc)
(import 'class/lisp/class.inc)

(def-method 'lisp 'func_cat)
	;inputs
	;r0 = lisp object (ptr)
	;r1 = args vector object (ptr)
	;outputs
	;r0 = lisp object (ptr)
	;r1 = return value object (ptr)

	(ptr 'this 'args 'value 'elem 'type)
	(pptr 'iter_begin 'iter_end)
	(uint 'length)

	(push-scope)
	(entry 'lisp 'func_cat {this, args})

	(d-call 'vector 'get_length {args} {_, length})
	(errorifnot {length} 'error1)
	(call 'vector 'get_iters {args} {_, iter_begin, iter_end})
	(assign {*iter_begin} {elem})
	(call 'lisp 'env_args_type (cat {args, @} (f-path 'string 'vtable) {, -1}) {type})
	(vpif {type})
		(vpif {length == 2})
			(call 'vector 'get_second {args} {_, value})
			(call 'string 'append {elem, value} {value})
		(else)
			(call 'string 'cat {args} {value})
		(endif)
	(else)
		(call 'lisp 'env_args_type (cat {args, @} (f-path 'vector 'vtable) {, -1}) {type})
		(vpif {type})
			(call 'vector 'create nil {value})
			(loop-while {iter_begin != iter_end})
				(assign {*iter_begin} {elem})
				(d-call 'vector 'get_length {elem} {_, length})
				(d-call 'vector 'append {value, elem, 0, length})
				(assign {iter_begin + ptr_size} {iter_begin})
			(loop-end)
		(else)
			(call 'lisp 'env_args_type (cat {args, @} (f-path 'array 'vtable) {, -1}) {type})
			(vpif {type})
				(call 'array 'create nil {value})
				(loop-while {iter_begin != iter_end})
					(assign {*iter_begin} {elem})
					(d-call 'array 'get_length {elem} {_, length})
					(d-call 'array 'append {value, elem, 0, length})
					(assign {iter_begin + ptr_size} {iter_begin})
				(loop-end)
			(else)
			(errorcases
				(call 'lisp 'env_args_type (cat {args, @} (f-path 'points 'vtable) {, -1}) {type})
				(errorifnot {type} 'error1))
				(call 'points 'create nil {value})
				(loop-while {iter_begin != iter_end})
					(assign {*iter_begin} {elem})
					(d-call 'points 'get_length {elem} {_, length})
					(d-call 'points 'append {value, elem, 0, length})
					(assign {iter_begin + ptr_size} {iter_begin})
				(loop-end)
			(endif)
		(endif)
	(endif)

(vp-label 'funcexit)
	(exit 'lisp 'func_cat {this, value})
	(return)

(errorcases
(vp-label 'error1)
	(call 'lisp 'repl_error {this, "(cat seq ...)", error_msg_wrong_types, args} {_, value})
	(goto 'funcexit))

	(pop-scope-syms)

(def-func-end)
