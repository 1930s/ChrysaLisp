(import 'sys/func.inc)
(import 'class/integer/integer.inc)
(import 'class/hash_map/hash_map.inc)
(import 'class/lisp/lisp.inc)

(def-func 'class/lisp/func_each)
	;inputs
	;r0 = lisp object (ptr)
	;r1 = args vector object (ptr)
	;outputs
	;r0 = lisp object (ptr)
	;r1 = return value object (ptr)

	(ptr 'this 'args 'value 'accum)
	(union
		'(ptr 'lists)
		'(ptr 'func)
		'(ptr 'type))
	(pptr 'iter 'iter_begin 'iter_end 'params)
	(int 'start 'end)
	(union
		'(int 'dir)
		'(uint 'seq_length))
	(uint 'length)

	(push-scope)
	(f-entry 'lisp 'func_each {this, args})
	(f-call 'lisp 'env_push {this})

	(d-call 'vector 'get_length {args} {_, length})
	(errorif {length != 5} 'error1)
	(f-call 'vector 'get_begin {args} {_, params})
	(assign {params[ptr_size * 4]} {lists})
	(errorif (cat {lists->obj_vtable != @} (f-path 'vector 'vtable)) 'error2)

	(assign {1000000} {length})
	(f-call 'vector 'get_iters {lists} {_, iter_begin, iter_end})
	(assign {iter_begin} {iter})
	(loop-while {iter != iter_end})
		(assign {*iter} {value})
		(f-call 'obj 'inst_of (cat {value, @} (f-path 'sequence 'vtable)) {_, type})
		(errorifnot {type} 'error3)
		(v-call 'sequence 'get_length {value} {_, seq_length})
		(vpif {seq_length < length})
			(assign {seq_length} {length})
		(endif)
		(assign {iter + ptr_size} {iter})
	(loop-end)

	(vpif {length != 1000000})
		(assign {0} {start})
		(assign {params[ptr_size * 0]} {value})
		(vpif {value != this->lisp_sym_nil})
			(errorif (cat {value->obj_vtable != @} (f-path 'integer 'vtable)) 'error4)
			(f-call 'integer 'get_value {value} {_, start})
		(endif)
		(assign {length} {end})
		(assign {params[ptr_size * 1]} {value})
		(vpif {value != this->lisp_sym_nil})
			(errorif (cat {value->obj_vtable != @} (f-path 'integer 'vtable)) 'error4)
			(f-call 'integer 'get_value {value} {_, end})
		(endif)
		(vpif {start < 0})
			(assign {length + start + 1} {start})
		(endif)
		(vpif {end < 0})
			(assign {length + end + 1} {end})
		(endif)
		(errorif {start < 0 || start > length || end < 0 || end > length} 'error4)
		(assign {1} {dir})
		(vpif {start > end})
			(assign {-1, start - 1, end - 1} {dir, start, end})
		(endif)

		(assign {params[ptr_size * 2]} {accum})
		(assign {params[ptr_size * 3]} {func})
		(f-call 'vector 'create nil {params})
		(loop-while {start != end})
			(assign {iter_begin} {iter})
			(loop-start)
				(v-call 'sequence 'ref_element {*iter, start} {_, value})
				(f-call 'vector 'push_back {params, value})
				(assign {iter + ptr_size} {iter})
			(loop-until {iter == iter_end})

			(f-call 'integer 'create {start} {value})
			(f-call 'hash_map 'insert {this->lisp_environment, this->lisp_sym_underscore, value})
			(f-call 'obj 'deref {value})

			(f-call 'lisp 'repl_apply {this, params, func} {_, value})
			(errorif (cat {value->obj_vtable == @} (f-path 'error 'vtable)) 'error5)
			(vpif {accum != this->lisp_sym_nil})
				(d-call 'vector 'clear {params})
				(f-call 'vector 'push_back {params, value})
				(f-call 'lisp 'repl_apply {this, params, accum} {_, value})
				(errorif (cat {value->obj_vtable == @} (f-path 'error 'vtable)) 'error5)
			(endif)
			(d-call 'vector 'clear {params})
			(f-call 'obj 'deref {value})
			(assign {start + dir} {start})
		(loop-end)
		(f-call 'vector 'deref {params})
	(endif)
	(f-call 'obj 'ref {this->lisp_sym_nil} {value})

(vp-label 'exit)
	(f-call 'lisp 'env_pop {this})
	(f-exit 'lisp 'func_each {this, value})
	(return)

(errorcases
(vp-label 'error1)
	(f-call 'lisp 'repl_error {this, "(each! start|nil end|nil lambda|nil lambda (seq ...))", error_msg_wrong_num_of_args, args} {_, value})
	(goto 'exit)
(vp-label 'error2)
	(f-call 'lisp 'repl_error {this, "(each! start|nil end|nil lambda|nil lambda (seq ...))", error_msg_not_a_list, args} {_, value})
	(goto 'exit)
(vp-label 'error3)
	(f-call 'lisp 'repl_error {this, "(each! start|nil end|nil lambda|nil lambda (seq ...))", error_msg_not_a_sequence, args} {_, value})
	(goto 'exit)
(vp-label 'error4)
	(f-call 'lisp 'repl_error {this, "(each! start|nil end|nil lambda|nil lambda (seq ...))", error_msg_not_valid_index, args} {_, value})
	(goto 'exit)
(vp-label 'error5)
	(f-call 'vector 'deref {params})
	(goto 'exit))

	(pop-scope-syms)

(def-func-end)
