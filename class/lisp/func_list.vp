(import 'sys/func.inc)
(import 'class/lisp/class.inc)

(def-method 'lisp 'func_list)
	;inputs
	;r0 = lisp object (ptr)
	;r1 = args vector object (ptr)
	;outputs
	;r0 = lisp object (ptr)
	;r1 = return value object (ptr)

	(ptr 'this 'args)

	(push-scope)
	(entry 'lisp 'func_list {this, args})

	(d-call 'vector 'slice {args, 0, args->array_length} {_, args})

	(exit 'lisp 'func_list {this, args})
	(pop-scope)
	(return)

(def-func-end)
