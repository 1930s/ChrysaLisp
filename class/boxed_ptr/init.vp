(import 'inc/func.inc)
(import 'class/class_boxed_ptr.inc)

(def-func 'class/boxed_ptr/init)
	;inputs
	;r0 = object
	;r1 = vtable pointer
	;outputs
	;r1 = 0 if error, else ok

	;init parent
	(s-call 'boxed_ptr 'init '(r0, r1) '(r1))
	(vpif '(r1 != 0))
		;init myself
		(vp-xor-rr r2 r2)
		(vp-cpy-ri r2 r0 boxed_ptr_value)
		(vp-cpy-ri r2 r0 boxed_ptr_flags)
	(endif)
	(vp-ret)

(def-func-end)
