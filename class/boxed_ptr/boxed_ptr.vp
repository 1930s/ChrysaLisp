(import 'inc/func.inc)
(import 'class/boxed_ptr/boxed_ptr.inc)

(class-macro-new 'boxed_ptr)
(class-macro-create 'boxed_ptr)

(def-func 'class/boxed_ptr/init)
	;inputs
	;r0 = object
	;r1 = vtable pointer
	;outputs
	;r1 = 0 if error, else ok

	;init myself
	(vp-xor-rr r2 r2)
	(vp-cpy-ri r2 r0 boxed_ptr_value)

	;init parent
	(s-jmp 'boxed_ptr 'init '(r0 r1))

(def-func-end)
