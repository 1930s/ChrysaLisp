(import 'inc/func.inc)
(import 'class/class_view.inc)

(def-func 'class/view/to_front)
	;inputs
	;r0 = view object
	;outputs
	;r0 = view object
	;trashes
	;r2

	;are we allready front ?
	ln_is_first r0 + view_node, r1
	(vpif '(r1 != 0))
		(f-call 'view 'add_front '(r0 (r0 view_parent)))
		(f-jmp 'view 'dirty_all '(r0))
	(endif)
	(vp-ret)

(def-func-end)
