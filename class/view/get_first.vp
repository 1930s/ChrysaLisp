(import 'inc/func.inc)
(import 'class/class_view.inc)

(def-func 'class/view/get_first)
	;inputs
	;r0 = view object
	;outputs
	;r0 = view object
	;r1 = 0 if empty, else first child
	;trashes
	;r2

	lh-get-head r0 + view_list, r1
	(ln-get-succ r1 r2)
	(vpif '(r2 == 0))
		(vp-cpy-rr r2 r1)
	(else)
		(vp-sub-cr view_node r1)
	(endif)
	(vp-ret)

(def-func-end)
