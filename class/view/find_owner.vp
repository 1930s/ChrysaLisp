(import 'inc/func.inc)
(import 'class/class_view.inc)

(def-func 'class/view/find_owner)
	;inputs
	;r0 = view object
	;outputs
	;r1 = 0, else tcb of owner
	;trashes
	;r2

	;walk up tree to parent
	(vp-cpy-rr r0 r2)
	loop_while r2, !=, 0
		(vp-cpy-ir r2 view_tcb r1)
		(vp-cpy-ir r2 view_parent r2)
		continueif r1, ==, 0
		(vp-ret)
	(loop-end)
	(vp-xor-rr r1 r1)
	(vp-ret)

(def-func-end)
