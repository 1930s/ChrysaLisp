(import 'inc/func.inc)
(import 'class/class_button.inc)

(def-func 'class/button/deinit)
	;inputs
	;r0 = button object
	;trashes
	;all but r0, r4

	;disconnnect all signals
	(f-call 'button 'disconnect_sig '(r0, &[r0 + button_pressed_signal]))

	;parent deinit
	s_jmp button, deinit, {r0}

(def-func-end)
