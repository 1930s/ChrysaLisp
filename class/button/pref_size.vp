(import 'inc/func.inc)
(import 'class/class_button.inc)

(def-func 'class/button/pref_size)
	;inputs
	;r0 = button object
	;outputs
	;r10 = prefered width
	;r11 = prefered height
	;trashes
	;all but r0, r4

	(s-call 'button 'pref_size '(r0) '(r10, r11))
	(vp-add-cr (mul button_border_size 2) r10)
	(vp-add-cr (mul button_border_size 2) r11)
	(vp-ret)

(def-func-end)
