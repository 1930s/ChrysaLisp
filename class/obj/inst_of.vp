(import 'inc/func.inc)
(import 'class/class_obj.inc)

(def-func 'class/obj/inst_of)
	;inputs
	;r0 = object
	;r1 = vtable pointer of tested type
	;outputs
	;r0 = object
	;r1 = 0 if not, else vtable of object
	;trashes
	;r2

	(vp-cpy-rr r1 r2)
	(loop-flist-forward r0 + obj_vtable, r1, r1)
	(loop-until '(r1 == r2))
	if r1, ==, r2
		(vp-cpy-ir r0 obj_vtable r1)
	endif
	(vp-ret)

(def-func-end)
