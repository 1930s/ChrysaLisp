(import 'inc/func.inc)
(import 'class/class_title.inc)

(def-func 'class/title/pref_size)
	;inputs
	;r0 = flow object
	;outputs
	;r10 = prefered width
	;r11 = prefered height
	;trashes
	;all but r0, r4

	(s-call 'title 'pref_size '(r0) '(r10 r11))
	(vp-add-cr (mul title_border_size 2) r10)
	(vp-add-cr (mul title_border_size 2) r11)
	(vp-ret)

(def-func-end)
