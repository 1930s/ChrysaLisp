(import 'inc/func.inc)
(import 'class/class_flow.inc)

(def-func 'class/flow/init)
	;inputs
	;r0 = flow object
	;r1 = vtable pointer
	;outputs
	;r1 = 0 if error, else ok

	;init parent
	(s-call 'flow 'init '(r0 r1) '(r1))
	(vpif '(r1 != 0))
		;init myself
		(vp-xor-rr r2 r2)
		(vp-cpy-ri r2 r0 flow_flags)
	(endif)
	(vp-ret)

(def-func-end)
