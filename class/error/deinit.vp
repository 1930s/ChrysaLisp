(import 'inc/func.inc)
(import 'class/class_error.inc)

(def-func 'class/error/deinit)
	;inputs
	;r0 = error object
	;trashes
	;all but r0, r4

	(vp-push r0)
	(f-call 'ref 'deref '((r0 error_description)))
	(vp-cpy-ir r4 r0)
	(f-call 'ref 'deref '((r0 error_object)))
	(vp-pop r0)
	(s-jmp 'error 'deinit '(r0))

(def-func-end)
