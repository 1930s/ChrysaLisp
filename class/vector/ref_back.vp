(import 'inc/func.inc)
(import 'class/class_vector.inc)

(def-func 'class/vector/ref_back)
	;inputs
	;r0 = vector object
	;outputs
	;r0 = vector object
	;r1 = object pointer

	(vp-push r0)
	(vp-cpy-ir r0 vector_length r1)
	(vp-cpy-ir r0 vector_array r0)
	(vp-mul-cr ptr_size r1)
	(vp-add-rr r1 r0)
	(f-call 'ref 'ref '(( r0 (sub 0 ptr_size )))
	(vp-cpy-rr r0 r1)
	(vp-pop r0)
	(vp-ret)

(def-func-end)
