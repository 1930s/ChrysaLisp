(import 'inc/func.inc)
(import 'inc/list.inc)

(def-func 'sys/list_index_of)
	;inputs
	;r0 = list head
	;r1 = list node
	;outputs
	;r0 = -1, else index
	;r1 = list node
	;trashes
	;r2, r3

	(lh-get-head r0 r2)
	(vp-xor-rr r0 r0)
	(loop-start)
		(vp-cpy-rr r2 r3)
		(ln-get-succ r2 r2)
		if r2, ==, 0
			(vp-cpy-cr -1 r0)
			(vp-ret)
		endif
		breakif r3, ==, r1
		(vp-inc r0)
	(loop-end)
	(vp-ret)

(def-func-end)
