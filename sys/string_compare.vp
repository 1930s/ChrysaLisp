(import 'inc/func.inc)

(def-func 'sys/string_compare)
	;inputs
	;r0 = string1
	;r1 = string2
	;outputs
	;r0 = 0 if same, else -, +
	;trashes
	;r0-r3

	(loop-start)
		(vp-cpy-ir-ub r0 r2)
		(vp-cpy-ir-ub r1 r3)
		(vp-sub-rr r3 r2)
		(breakif '(r2 != 0))
		(breakif '(r3 == 0))
		(vp-inc r0)
		(vp-inc r1)
	(loop-end)
	(vp-cpy-rr r2 r0)
	(vp-ret)

(def-func-end)
