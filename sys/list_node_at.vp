(import 'inc/func.inc)
(import 'inc/list.inc)

(def-func 'sys/list_node_at)
	;inputs
	;r0 = list head
	;r1 = index
	;outputs
	;r0 = 0, else list node
	;trashes
	;r1, r2

	(lh-get-head r0 r2)
	(loop-start)
		(vp-cpy-rr r2 r0)
		(ln-get-succ r2 r2)
		if r2, ==, 0
			(vp-xor-rr r0 r0)
			(vp-ret)
		endif
		breakif r1, ==, 0
		(vp-dec r1)
	(loop-end)
	(vp-ret)

(def-func-end)
